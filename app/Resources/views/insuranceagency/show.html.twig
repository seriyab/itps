{% extends 'base.html.twig' %}

{% block body %}
    <h1>Insuranceagency</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ insuranceAgency.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ insuranceAgency.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ insuranceAgency.address }}</td>
            </tr>
            <tr>
                <th>Phonenumber</th>
                <td>{{ insuranceAgency.phoneNumber }}</td>
            </tr>
            <tr>
                <th>Faxnumber</th>
                <td>{{ insuranceAgency.faxNumber }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('insuranceagency_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('insuranceagency_edit', { 'id': insuranceAgency.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
